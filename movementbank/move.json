{
  "name": "forward_stable",
  "keyframes": [
    // Étape 1 : Groupe 1 (avant gauche, milieu droit, arrière gauche) se lève
    {
      "3": 0.5, "4": 0.75,  // Avant gauche : levée
      "5": 0.5, "6": 0.75,  // Milieu droit : levée
      "11": 0.5, "12": 0.75, // Arrière gauche : levée
      "1": 1.0, "2": 1.0,   // Avant droite : au sol
      "7": 1.0, "8": 1.0,   // Milieu gauche : au sol
      "9": 1.0, "10": 1.0   // Arrière droite : au sol
    },
    // Étape 2 : Groupe 1 avance
    {
      "3": 0.7, "4": 0.6,   // Avant gauche : avancée
      "5": 0.7, "6": 0.6,   // Milieu droit : avancée
      "11": 0.7, "12": 0.6, // Arrière gauche : avancée
      "1": 1.0, "2": 1.0,   // Groupe 2 reste au sol
      "7": 1.0, "8": 1.0,
      "9": 1.0, "10": 1.0
    },
    // Étape 3 : Groupe 1 repose, Groupe 2 (avant droite, milieu gauche, arrière droite) se lève
    {
      "3": 1.0, "4": 1.0,   // Avant gauche : repose
      "5": 1.0, "6": 1.0,   // Milieu droit : repose
      "11": 1.0, "12": 1.0, // Arrière gauche : repose
      "1": 0.5, "2": 0.75,  // Avant droite : levée
      "7": 0.5, "8": 0.75,  // Milieu gauche : levée
      "9": 0.5, "10": 0.75  // Arrière droite : levée
    },
    // Étape 4 : Groupe 2 avance
    {
      "3": 1.0, "4": 1.0,   // Groupe 1 reste au sol
      "5": 1.0, "6": 1.0,
      "11": 1.0, "12": 1.0,
      "1": 0.7, "2": 0.6,   // Avant droite : avancée
      "7": 0.7, "8": 0.6,   // Milieu gauche : avancée
      "9": 0.7, "10": 0.6   // Arrière droite : avancée
    }
  ]
}
